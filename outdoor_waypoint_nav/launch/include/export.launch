<?xml version="1.0"?>
<launch>
  <arg name="file"     default="$(find outdoor_waypoint_nav)/bags/iribe_wt_path.bag"/>
  <node pkg="rosbag" type="play" name="rosbag" required="true" args="$(arg file)"/>
  <node name="extract" pkg="image_view" type="extract_images" respawn="false" required="true" output="screen" cwd="ROS_HOME">
    <remap from="image" to="/camera/color/image_raw"/>
  </node>
</launch>