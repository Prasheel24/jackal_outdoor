 <node pkg="nodelet" type="nodelet" name="nodelet_manager" args="manager"/>
 <node pkg="nodelet" type="nodelet" name="velocity_smoother" args="load yocs_velocity_smoother/VelocitySmootherNodelet nodelet_manager" output="screen">
    <rosparam file="$(find outdoor_waypoint_nav)/params/smoother.yaml" command="load"/>
    <remap from="velocity_smoother/odometry" to="odometry/filtered"/>
    <remap from="velocity_smoother/robot_cmd_vel" to="jackal_velocity_controller/cmd_vel"/>

    <!-- <remap from="velocity_smoother/raw_cmd_vel" to="jackal_velocity_controller/cmd_vel"/> -->
    <remap from="velocity_smoother/smooth_cmd_vel" to="/jackal_velocity_controller/cmd_vel"/>
    <param name="velocity_maximum" value="0.50"/>
  </node> 
</launch>